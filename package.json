{
  "name": "prob-tracker",
  "version": "1.0.0",
  "description": "A simple problem tracker and catalogue",
  "private": false,
  "main": "./bin/www",
  "scripts": {
    "start": "gulp serve",
    "build": "gulp"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/register": "^7.0.0-beta.47",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "chai": "^4.1.2",
    "cross-env": "^5.1.5",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "mocha": "^5.2.0",
    "mockery": "^2.1.0",
    "nodemon": "^1.17.5-alpha.2",
    "rewire": "^4.0.1",
    "run-sequence": "^2.2.1",
    "supertest": "^3.1.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-add-module-exports",
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrazyBot/prob-tracker.git"
  },
  "author": "Elliot Warren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KrazyBot/prob-tracker/issues"
  },
  "homepage": "https://github.com/KrazyBot/prob-tracker#readme",
  "dependencies": {
    "comunik8-common": "^2.0.29",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^3.2.5",
    "monk": "^7.0.0",
    "morgan": "~1.9.1"
  }
}
